int led2 = D7; // Instead of writing D7 over and over again, we'll write led2
void setup() {
  pinMode(led2, OUTPUT);

}

void loop() {

  // H
  digitalWrite(led2, HIGH);
  delay(500);
  digitalWrite(led2, LOW);
  delay(500);
