  digitalWrite(led2, HIGH);
  delay(500);
  digitalWrite(led2, LOW);
  delay(500);
    // .
  digitalWrite(led2, HIGH);
  delay(500);
  digitalWrite(led2, LOW);
  delay(500);
    // .
  digitalWrite(led2, HIGH);
  delay(500);
  digitalWrite(led2, LOW);
  
  delay(2000);

  // U
  digitalWrite(led2, HIGH);
  delay(500);
  digitalWrite(led2, LOW);
  delay(500);
  // .
  digitalWrite(led2, HIGH);
  delay(500);
  digitalWrite(led2, LOW);
  delay(500);
  // -
  digitalWrite(led2, HIGH);
  delay(1000);
  digitalWrite(led2, LOW);
  
  delay(2000);
  
    // G
  digitalWrite(led2, HIGH);
  delay(1000);
